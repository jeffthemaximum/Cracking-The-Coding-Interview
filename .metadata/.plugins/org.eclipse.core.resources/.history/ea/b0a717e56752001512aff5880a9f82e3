
public class Node {
	Object data;
	Node next = null;
	
	public Node(Object item){
		data = item;
	}
}

class Stack {
	Node top;
	
	Object pop() {
		if (top != null) {
			Object item = top.data;
			top = top.next;
			return item;
		}
		return null;
	}
	
	void push(Object item) {
		Node t = new Node(item);
		t.next = top;
		top = t;
	}
	
	void peek() {
		return top.data;
	}
}


